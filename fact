package practice;

import java.util.Scanner;
//import java.util.io*;

public class factorial {
    int a;
    public static void main(String[] args) {
        System.out.println("enter the number ");
        Scanner a =new Scanner(System.in);
        int b=1;
        int v=a.nextInt();
        for(int i=1;i<=v;i++){
            b=b*i;
        }
        System.out.println("value of factorial is "+ b);

    }
}
